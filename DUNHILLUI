-- üìä Stats Cards
local function CreateStatCard(title, value, icon, yPos)
	local Card = Instance.new("Frame")
	Card.Size = UDim2.new(0.48, 0, 0, 80)
	Card.Position = UDim2.new((yPos % 2) * 0.52, 0, 0, 90 + math.floor(yPos / 2) * 90)
	Card.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
	Card.BorderSizePixel = 0
	Card.Parent = Home
	
	local CardCorner = Instance.new("UICorner")
	CardCorner.CornerRadius = UDim.new(0, 12)
	CardCorner.Parent = Card
	
	local CardIcon = Instance.new("TextLabel")
	CardIcon.Text = icon
	CardIcon.Size = UDim2.new(0, 40, 0, 40)
	CardIcon.Position = UDim2.new(0, 15, 0, 10)
	CardIcon.BackgroundTransparency = 1
	CardIcon.TextColor3 = Color3.fromRGB(138, 43, 226)
	CardIcon.Font = Enum.Font.GothamBold
	CardIcon.TextSize = 24
	CardIcon.Parent = Card
	
	local CardTitle = Instance.new("TextLabel")
	CardTitle.Text = title
	CardTitle.Size = UDim2.new(1, -70, 0, 20)
	CardTitle.Position = UDim2.new(0, 15, 0, 52)
	CardTitle.BackgroundTransparency = 1
	CardTitle.TextColor3 = Color3.fromRGB(150, 150, 160)
	CardTitle.Font = Enum.Font.Gotham
	CardTitle.TextSize = 11
	CardTitle.TextXAlignment = Enum.TextXAlignment.Left
	CardTitle.Parent = Card
	
	local CardValue = Instance.new("TextLabel")
	CardValue.Text = value
	CardValue.Size = UDim2.new(0, 100, 0, 25)
	CardValue.Position = UDim2.new(0, 65, 0, 15)
	CardValue.BackgroundTransparency = 1
	CardValue.TextColor3 = Color3.fromRGB(255, 255, 255)
	CardValue.Font = Enum.Font.GothamBold
	CardValue.TextSize = 18
	CardValue.TextXAlignment = Enum.TextXAlignment.Left
	CardValue.Parent = Card
	
	return Card
end

