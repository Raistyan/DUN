-- üåü PREMIUM EXECUTOR UI | Modern Hub Style
-- Inspired by Chiyo Hub & Vinzz Hub
-- Compatible with Delta / Xeno / Arceus X / Fluxus
-- Features: Script Hub, Executor, Games Library, Settings

local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerGui = player:FindFirstChildOfClass("PlayerGui") or player:WaitForChild("PlayerGui")

-- üé® Theme Colors
local Theme = {
    Background = Color3.fromRGB(12, 12, 16),
    Secondary = Color3.fromRGB(18, 18, 24),
    Accent = Color3.fromRGB(88, 101, 242), -- Discord Blue
    AccentHover = Color3.fromRGB(108, 121, 255),
    Text = Color3.fromRGB(255, 255, 255),
    TextDim = Color3.fromRGB(180, 180, 190),
    Success = Color3.fromRGB(67, 181, 129),
    Warning = Color3.fromRGB(250, 166, 26),
    Danger = Color3.fromRGB(237, 66, 69),
}

-- üß± GUI Root
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "PremiumExecutorUI"
ScreenGui.ResetOnSpawn = false
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = playerGui

-- ü™ü Main Window (Lebih besar dan modern)
local MainFrame = Instance.new("Frame")
MainFrame.Name = "MainFrame"
MainFrame.Size = UDim2.new(0, 650, 0, 420)
MainFrame.Position = UDim2.new(0.5, -325, 0.5, -210)
MainFrame.BackgroundColor3 = Theme.Background
MainFrame.BorderSizePixel = 0
MainFrame.Active = true
MainFrame.Draggable = true
MainFrame.ClipsDescendants = true
MainFrame.Parent = ScreenGui

local MainCorner = Instance.new("UICorner")
MainCorner.CornerRadius = UDim.new(0, 16)
MainCorner.Parent = MainFrame

-- ‚ú® Glow Effect
local Glow = Instance.new("ImageLabel")
Glow.Name = "Glow"
Glow.AnchorPoint = Vector2.new(0.5, 0.5)
Glow.Position = UDim2.new(0.5, 0, 0.5, 0)
Glow.Size = UDim2.new(1, 50, 1, 50)
Glow.BackgroundTransparency = 1
Glow.Image = "rbxassetid://1316045217"
Glow.ImageColor3 = Theme.Accent
Glow.ImageTransparency = 0.85
Glow.ScaleType = Enum.ScaleType.Slice
Glow.SliceCenter = Rect.new(10, 10, 118, 118)
Glow.ZIndex = 0
Glow.Parent = MainFrame

