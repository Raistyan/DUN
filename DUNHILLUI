ExecuteBtn.MouseButton1Click:Connect(function()
	local success, err = pcall(function()
		loadstring(ScriptBox.Text)()
	end)
	if success then
		ExecuteBtn.Text = "‚úì EXECUTED!"
		wait(1)
		ExecuteBtn.Text = "‚ö° EXECUTE"
	else
		ExecuteBtn.Text = "‚úó ERROR"
		wait(1)
		ExecuteBtn.Text = "‚ö° EXECUTE"
	end
end)

