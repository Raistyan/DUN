	ColorSequenceKeypoint.new(0, Color3.fromRGB(138, 43, 226)),
	ColorSequenceKeypoint.new(0.5, Color3.fromRGB(255, 20, 147)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(138, 43, 226))
})
BorderGradient.Parent = AccentBorder

-- üî• Animated Gradient Border
spawn(function()
	while AccentBorder and AccentBorder.Parent do
		smoothTween(BorderGradient, {Offset = Vector2.new(1, 0)}, 2)
		wait(2)
		BorderGradient.Offset = Vector2.new(-1, 0)
	end
end)

