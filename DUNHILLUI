-- üåü Tab Creation Function dengan Icon
local currentTab = nil
local function CreateTab(name, icon)
	local Tab = Instance.new("TextButton")
	Tab.Name = name
	Tab.Text = ""
	Tab.Size = UDim2.new(1, -20, 0, 45)
	Tab.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
	Tab.BorderSizePixel = 0
	Tab.AutoButtonColor = false
	Tab.Parent = TabHolder
	
	local TabCorner = Instance.new("UICorner")
	TabCorner.CornerRadius = UDim.new(0, 10)
	TabCorner.Parent = Tab

	local TabIcon = Instance.new("TextLabel")
	TabIcon.Text = icon
	TabIcon.Size = UDim2.new(0, 25, 0, 25)
	TabIcon.Position = UDim2.new(0, 10, 0.5, -12)
	TabIcon.BackgroundTransparency = 1
	TabIcon.TextColor3 = Color3.fromRGB(180, 180, 190)
	TabIcon.Font = Enum.Font.GothamBold
	TabIcon.TextSize = 18
	TabIcon.Parent = Tab

	local TabLabel = Instance.new("TextLabel")
	TabLabel.Text = name
	TabLabel.Size = UDim2.new(1, -45, 1, 0)
	TabLabel.Position = UDim2.new(0, 40, 0, 0)
	TabLabel.BackgroundTransparency = 1
	TabLabel.TextColor3 = Color3.fromRGB(180, 180, 190)
	TabLabel.Font = Enum.Font.GothamSemibold
	TabLabel.TextSize = 14
	TabLabel.TextXAlignment = Enum.TextXAlignment.Left
	TabLabel.Parent = Tab

	-- Hover Effects
	Tab.MouseEnter:Connect(function()
		if currentTab ~= Tab then
			smoothTween(Tab, {BackgroundColor3 = Color3.fromRGB(35, 35, 45)}, 0.2)
			smoothTween(Tab, {Size = UDim2.new(1, -15, 0, 45)}, 0.2)
		end
	end)

	Tab.MouseLeave:Connect(function()
		if currentTab ~= Tab then
			smoothTween(Tab, {BackgroundColor3 = Color3.fromRGB(25, 25, 35)}, 0.2)
			smoothTween(Tab, {Size = UDim2.new(1, -20, 0, 45)}, 0.2)
		end
	end)

	Tab.MouseButton1Click:Connect(function()
		-- Reset all tabs
		for _, button in pairs(TabHolder:GetChildren()) do
			if button:IsA("TextButton") then
				smoothTween(button, {BackgroundColor3 = Color3.fromRGB(25, 25, 35)}, 0.3)
				smoothTween(button, {Size = UDim2.new(1, -20, 0, 45)}, 0.2)
				local icon = button:FindFirstChild("TextLabel")
				local label = button:FindFirstChild("TextLabel", true)
				if icon then smoothTween(icon, {TextColor3 = Color3.fromRGB(180, 180, 190)}, 0.3) end
			end
		end
		
		-- Activate clicked tab
		currentTab = Tab
		smoothTween(Tab, {BackgroundColor3 = Color3.fromRGB(138, 43, 226)}, 0.3)
		smoothTween(TabIcon, {TextColor3 = Color3.fromRGB(255, 255, 255)}, 0.3)
		smoothTween(TabLabel, {TextColor3 = Color3.fromRGB(255, 255, 255)}, 0.3)
		
		-- Switch pages
		for _, frame in pairs(ContentFrame:GetChildren()) do
			if frame:IsA("Frame") or frame:IsA("ScrollingFrame") then
				frame.Visible = false
			end
		end
		local page = ContentFrame:FindFirstChild(name)
		if page then
			page.Visible = true
			page.GroupTransparency = 1
			smoothTween(page, {GroupTransparency = 0}, 0.4)
		end
	end)
	
	return Tab
end

-- ü™Ñ Create Tabs dengan Icons
CreateTab("Home", "üè†")
CreateTab("Executor", "‚ö°")
CreateTab("Scripts", "üìú")
CreateTab("Settings", "‚öôÔ∏è")

