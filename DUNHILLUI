local function CreateStat(text, value, color)
    local stat = Instance.new("Frame")
    stat.BackgroundColor3 = Color3.fromRGB(20, 20, 28)
    stat.BorderSizePixel = 0
    stat.Parent = statsGrid
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 10)
    corner.Parent = stat
    
    local valueLabel = Instance.new("TextLabel")
    valueLabel.Text = value
    valueLabel.Size = UDim2.new(1, 0, 0.5, 0)
    valueLabel.BackgroundTransparency = 1
    valueLabel.TextColor3 = color
    valueLabel.Font = Enum.Font.GothamBold
    valueLabel.TextSize = 20
    valueLabel.Parent = stat
    
    local textLabel = Instance.new("TextLabel")
    textLabel.Text = text
    textLabel.Size = UDim2.new(1, 0, 0.5, 0)
    textLabel.Position = UDim2.new(0, 0, 0.5, 0)
    textLabel.BackgroundTransparency = 1
    textLabel.TextColor3 = Theme.TextDim
    textLabel.Font = Enum.Font.Gotham
    textLabel.TextSize = 11
    textLabel.Parent = stat
end

CreateStat("SCRIPTS", "150+", Theme.Success)
CreateStat("GAMES", "50+", Theme.Warning)
CreateStat("USERS", "10K+", Theme.Accent)

-- ‚ö° EXECUTOR PAGE
local editorSection = CreateSection(ExecutorPage, "Script Editor")
local scriptBox = Instance.new("TextBox")
scriptBox.Size = UDim2.new(1, 0, 0, 200)
scriptBox.BackgroundColor3 = Color3.fromRGB(10, 10, 15)
scriptBox.Text = "-- Enter your script here\nprint('Hello from Premium Executor!')"
scriptBox.TextColor3 = Theme.Text
scriptBox.Font = Enum.Font.Code
scriptBox.TextSize = 13
scriptBox.TextXAlignment = Enum.TextXAlignment.Left
scriptBox.TextYAlignment = Enum.TextYAlignment.Top
scriptBox.MultiLine = true
scriptBox.ClearTextOnFocus = false
scriptBox.BorderSizePixel = 0
scriptBox.Parent = editorSection

local boxCorner = Instance.new("UICorner")
boxCorner.CornerRadius = UDim.new(0, 10)
boxCorner.Parent = scriptBox

local boxPadding = Instance.new("UIPadding")
boxPadding.PaddingTop = UDim.new(0, 10)
boxPadding.PaddingLeft = UDim.new(0, 10)
boxPadding.PaddingRight = UDim.new(0, 10)
boxPadding.PaddingBottom = UDim.new(0, 10)
boxPadding.Parent = scriptBox

CreateButton(editorSection, "‚ñ∂ Execute Script", Theme.Success, function()
    local success, err = pcall(function()
        loadstring(scriptBox.Text)()
    end)
    if not success then
        warn("Execution error:", err)
    end
end)

CreateButton(editorSection, "üóë Clear", Theme.Danger, function()
    scriptBox.Text = ""
end)

-- üìú SCRIPTS PAGE
local scriptHubSection = CreateSection(ScriptsPage, "Script Hub")
